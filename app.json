{
  "expo": {
    "name": "Type Master",
    "slug": "type-master",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/imgs/typemaster_splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.butalsoverycool.kimnkoubou",
      "buildNumber": "1.0.0",
      "requireFullScreen": true
    },
    "android": {
      "package": "com.butalsoverycool.kimnkoubou",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "svg", "svgx"]
    }
  }
}
